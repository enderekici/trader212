{
  "name": "trader212",
  "version": "1.0.0",
  "type": "module",
  "description": "Autonomous AI-powered stock trading bot for Trading212",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "keywords": [
    "trading",
    "trading212",
    "algorithmic-trading",
    "autonomous-trading",
    "ai-trading",
    "technical-analysis",
    "sentiment-analysis"
  ],
  "author": "Ender Ekici",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "axios": "^1.13.5",
    "axios-retry": "^4.5.0",
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "node-cron": "^4.2.1",
    "node-telegram-bot-api": "^0.67.0",
    "pino": "^10.3.1",
    "technicalindicators": "^3.1.0",
    "ws": "^8.19.0",
    "yahoo-finance2": "^3.13.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.18",
    "drizzle-kit": "^0.31.9",
    "pino-pretty": "^13.1.3",
    "supertest": "^7.2.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "request-promise-core": {
      "request": "npm:@cypress/request@^3.0.10"
    },
    "@esbuild-kit/core-utils": {
      "esbuild": "^0.25.0"
    },
    "test-exclude": {
      "glob": "^13.0.0"
    }
  }
}
