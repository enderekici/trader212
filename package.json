{
  "name": "trader212",
  "version": "1.0.0",
  "type": "module",
  "description": "Autonomous AI-powered stock trading bot for Trading212",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "keywords": [
    "trading",
    "trading212",
    "algorithmic-trading",
    "autonomous-trading",
    "ai-trading",
    "technical-analysis",
    "sentiment-analysis"
  ],
  "author": "Ender Ekici",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "better-sqlite3": "^11.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.3",
    "express": "^4.21.2",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.67.0",
    "pino": "^9.6.0",
    "technicalindicators": "^3.1.0",
    "ws": "^8.18.0",
    "yahoo-finance2": "^2.14.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/ws": "^8.5.14",
    "@vitest/coverage-v8": "^3.0.5",
    "drizzle-kit": "^0.31.9",
    "pino-pretty": "^13.0.0",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "overrides": {
    "request-promise-core": {
      "request": "npm:@cypress/request@^3.0.10"
    },
    "@esbuild-kit/core-utils": {
      "esbuild": "^0.25.0"
    },
    "test-exclude": {
      "glob": "^13.0.0"
    }
  }
}
